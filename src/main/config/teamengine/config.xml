<?xml version="1.0" encoding="UTF-8"?>
<!-- Unpack the *-ctl-scripts.zip archive in the TE_BASE/scripts directory -->
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>NSG WFS 2.0 Profile</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/wfs20-nsg</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-wfs20-nsg-${version}</local-name>
        <title>Test suite ets-wfs20-nsg</title>
        <description>Describe scope of testing.</description>
        <BasicConformanceClasses>
          <conformanceClass>Preconditions</conformanceClass>
          <conformanceClass>All GML application schemas</conformanceClass>
          <conformanceClass>GML application schemas defining features</conformanceClass>
          <conformanceClass>NSG Basic WFS Conformance Class</conformanceClass>
          <conformanceClass>NSG Locking WFS</conformanceClass>
          <conformanceClass>Remote Resolve</conformanceClass>
          <conformanceClass>NSG Manage Stored Queries</conformanceClass>
          <conformanceClass>Enhanced Paging</conformanceClass>
        </BasicConformanceClasses>
      </suite>
      <revision>
        <name>${version}</name>
        <status>Beta</status>
        <sources>
          <source>wfs20-nsg/${spec-version}/ctl</source>
        </sources>
        <webdir>wfs20-nsg/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
</organization>
